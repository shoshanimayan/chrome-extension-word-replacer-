{
"manifest_version": 2,
    "name": "word replacer",
    "description": "a simple project to learn the chrome extension format",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "replace.js"
            ],
            "run_at": "document_end"
        }
    ]
}